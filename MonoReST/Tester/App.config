<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="restconfig" type="System.Configuration.NameValueSectionHandler"/>
		<section name="resttests" type="System.Configuration.NameValueSectionHandler"/>
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
	</startup>
	<!-- There is another, duplicated section below for non-windows configurations due to platform differences
       in fetching the NameValueSectionHandler. Hopefully this will be resolved in .netcore release but 
       so far is required for Mono
  -->
	<restconfig>
		<!-- If set to true, windows form(s) with textboxes will show for each thread so 
         each thread is logging (on the screen) to its own output-->
		<!-- This does not work with Mono, not sure about .netcore yet so leave to false if not Windows.Net -->
		<add key="useformlogging" value="false"/>
		<!-- When running, each exported/viewed file will be opened in the native application.
         This is not recommended for large amounts of document/threads-->
		<add key="openeachfile" value="false"/>
		<!-- Will open a window (explorer) to show the downloaded/viewed files directory-->
		<add key="showdownloadedfiles" value="false"/>
		<!-- NOTE: For all the directory locations below, you can specify an absolute
         path like C:\Dir1\dir2  or relative testfiles in the case of specifying a 
         relative path like "testfiles", it will expect testfiles to exist in the
         same location as the executable -->
		<!-- The directory to be used to gain random sampling of files from-->
		<add key="importFilesDirectory" value="c:\temp\Import\Files"/>
		<add key="importEmailsDirectory" value="c:\temp\Import\Emails"/>
		<!-- The base directory for all test output, if see note above about absolute
         vs. relative paths -->
		<add key="exportFilesDirectory" value="D:\AVEVA_DATA\AGCC\PreProcessor\Import\03_ProjectDocuments"/>
		<!-- If you want performance data logged to the console or not. -->
		<add key="performancedatatoconsole" value="true"/>
		<add key="defaultRestHomeUri" value="https://s502as-isd-ap5.sibur.local:5443/dctm-rest.72P33/services"/>
		<add key="defaultUsername" value="srvaveva"/>
		<add key="defaultPassword" value="D#1LhxQz"/>
		<add key="defaultRepositoryName" value="ICENTER_P"/>
		<add key="defaultPrintResult" value="true"/>
		<add key="LogThreshold" value="DEBUG"/>
		<add key="generateCSV" value="true"/>
		<add key="generateJSON" value="true"/>
		<add key="getDelta" value="false"/>
		<add key="timeStampfilePath" value="D:\AVEVA_DATA\AGCC\Configuration\CMIS\02_ProjectDocuments"/>
		<add key="DQL" value="select doc.arctic_document_class,
doc.arctic_document_title_eng,
doc.arctic_document_title,
doc.sib_doc_type_code,
doc.sib_discipline_code,
doc.sib_contr_transm_inc_number,
doc.eif_issue_reason,
doc.eif_acceptance_code,
doc.arctic_mark_stamp,
doc.eif_revision,
doc.sib_unit_title,
doc.r_object_id, 
doc.object_name,  
doc.r_modify_date,
ctrm.object_name as CTRM_number, 
ctrm.sib_tr_stage as CTRM_stage,
ctrm.eif_date_sent as CTRM_date,
itrm.object_name as iTRM_number, 
itrm.sib_tr_stage as iTRM_stage,
itrm.eif_date_sent as iTRM_date,
ctrm.eif_tr_category as ctrm_category,
itrm.eif_tr_category as itrm_category

from 
eifx_deliverable_doc doc 
left join eif_tr_doc_relation etdr on doc.r_object_id = etdr.child_id
left join eifx_transmittal ctrm on ctrm.r_object_id =  etdr.parent_id and ctrm.sib_tr_stage ='CTRM' and ctrm.eif_tr_category = 'outgoing'
left join eifx_transmittal itrm on itrm.r_object_id =  etdr.parent_id and itrm.sib_tr_stage ='ITRM' and itrm.eif_tr_category = 'outgoing'
where eif_project_ref = 'AGCC'
and (doc.object_name like 'AGCC.323%' or doc.object_name like 'AGCC.287%' or doc.object_name like 'AGCC.417%') and (doc.object_name not like '%RFP%') and (doc.object_name not like '%RFQ%') and (doc.object_name not like '%TBT%') and (doc.object_name not like '%ТВТ%') and (doc.object_name not like '%РКД%') and (doc.object_name not like '%RKD%') and (doc.object_name not like '%TBE%') and (doc.object_name not like '%TQS%') and (doc.object_name not like '%TCP%') and (doc.object_name not like '%OTP%') and (doc.object_name not like '%ОТР%') and (doc.object_name not like '%-ТТ-%')  and (doc.object_name not like '%-ТТ-%')and (doc.object_name not like '%ТЭО%') and (doc.object_name not like '%-ТХ-%') and (doc.object_name not like '%TQS-%') and (doc.object_name not like '%ТЭР%') and (doc.object_name not like '%ИТТ%') and (doc.object_name not like '%ИГИ%')
and (ctrm.object_name != '' or itrm.object_name != '')							
"/>
		<add key="itemsPerPage" value="35000"/>
